<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
      <ul>
      <li><%= link_to "Activate", "/articles/process?id=1&nextAction=activate" %></li>
      <li><%= link_to "Deactivate", "/articles/process?id=1&nextAction=deactivate" %></li>
      </ul>
    </li>
  <% end %>
</ul>

<%= link_to "New Article", new_article_path %>
<div>
<%=link_to 'Log Out', sessions_path, method: :delete%>
</div>